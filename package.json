    "generate": "npm run generate:inspector && npm run generate:duckkey && npm run generate:duckarchive",
    "new:inspector": "prisma migrate dev --config prisma-inspector.config.ts",
    "new:duckkey": "prisma migrate dev --config prisma-duckkey.config.ts",
    "new:duckarchive": "prisma migrate dev --config prisma-duckarchive.config.ts"
